import java.util.*;
public class Temparature {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter Celsius temp: ");
        int cel = sc.nextInt();
        System.out.println("Enter Fahrenheit temp: ");
        int fah = sc.nextInt();
        System.out.println("Celsius = " + cel);
        System.out.println("Fahrenheit = " + fah);
        System.out.println("Celsius to Fahrenheit: " + (1.8 * cel + 32));
        System.out.println("Fahrenheit to Celsius: " + ((fah - 32)*0.5556));
    }
}
