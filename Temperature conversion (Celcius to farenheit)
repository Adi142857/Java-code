import java.util.Scanner;
class Convert{
	public static void main(String args[])
	{		
		Scanner userin = new Scanner(System.in);
		System.out.println("Enter Temperature in Celsius: "); //enter temp in celcius
		int input =userin.nextInt();	
		int fahr=((9*input)/5)+32;
		int kel = input+273.15f;
		System.out.println("Temperature in Celsius: "+input +"\nTemperature in Fahreinheit: "+fahr+"\nTemperature in Kelvin: "+kel);
	}
	
}
